<img src="assets/legit_hub_top_left.png" id="top_left_image" />
<%= link_to "Dashboard", dashboard_path, id: "dashboard_link", class: "btn" %>
<div class="container-fluid center-align">
  <h3 id="my_commits_header" style="opacity:0;">Recent Commits:</h3>
  <ul class="collection" id="my_commits_list" style="opacity:0;">
    <%= render partial: "commits_item", collection: (@presenter.recent_commits(@current_user)), as: :commit %>
  </ul>
</div>
<div class="container-fluid center-align">
  <h3 id="follower_commits_header" style="opacity:0;">Following Commits:</h3>
  <ul class="collection" id="follower_commits_list" style="opacity:0;">
    <%= render partial: "following_commits_item", collection: (@presenter.following_commits), as: :commit %>
  </ul>
</div>
<img src="assets/legit_hub_bottom_right.png" id="bottom_right_image"/>
